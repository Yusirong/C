#include<stdio.h>

int main()
{
  float r, h, c, s1, s2, v1, v2;
	float pi = 3.1415926;
	printf("请输入半径和高:");
	scanf("%f%f", &r, &h);
	c = 2 * pi * r;
	s1 = pi * r * r;
	s2 = 4 * pi * r * r;
	v1 = 4.0/3.0 * pi * r * r * r;
	v2 = pi * r * r * h;
	printf("圆柱体积v2=%f\n", c, s1, s2, v1, v2);
  return 0;
}
