#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
int main()
{
  char grade;
	scanf("%c", &grade);
	printf("grade:%c\n", grade);
	switch (grade)
	{
		case 'A':printf("score:85-100"); break;
		case 'B':printf("score:70-84"); break;
		case 'C':printf("score:60-69"); break;
		case 'D':printf("score:0-60"); break;
		default:printf("enter wrong data!\n");
    }
	return 0;
}
